<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.PuyoPuzzle">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="puyopuyo"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".GameMainActivity"></activity>
        <activity android:name=".GameStartActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name=".MainActivityForServer"></activity>
        <activity android:name=".MainActivityForClient"></activity>
        <activity android:name=".RoomActivity"></activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET"/>

</manifest>